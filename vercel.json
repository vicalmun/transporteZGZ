{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "style.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/bus",
      "dest": "/public/bus.html"
    },
    {
      "src": "/tram",
      "dest": "/public/tram.html"
    },
    {
      "src": "/",
      "dest": "/public/bus.html"
    }
  ]
}